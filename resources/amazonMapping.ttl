@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix schema: <http://schema.org/>.
@prefix fno: <http://semweb.datasciencelab.be/ns/function#> .
@prefix semantic: <http://sematics.tuwien.ac.at/group4#>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .

<#SongMapping>
rml:logicalSource [
        rml:source "DSAR_Dominik_Scheffknecht_Mein_Song_Verlauf_TestSet.csv";
        rml:referenceFormulation ql:CSV ;
];
rr:subjectMap [
    rr:template "http://sematics.tuwien.ac.at/group4/tracks/{title}-{artistAsin}";
    rr:class semantic:AmazonMusicSong
];

rr:predicateObjectMap [
    rr:predicate schema:creditedTo;
    rr:objectMap [
    rml:reference "artistAsin"
    ]
];

rr:predicateObjectMap [
    rr:predicate schema:Duration;
    rr:objectMap [
    rml:reference "consumptionDurationMs"
    ]
].

